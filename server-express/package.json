{
  "name": "angular2-full-stack",
  "version": "9.1.0",
  "license": "MIT",
  "author": "Davide Violante",
  "description": "Angular Full Stack project built using Angular 2+, Express, Mongoose and Node.",
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"tsc -w -p server\" \"nodemon dist/server/server-express/server/app.js\"",
    "prod": "concurrently \"mongod\" \" && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "linthtml": "htmlhint \"client/**/*.html\"",
    "lintscss": "sass-lint \"client/**/*.scss\" -v",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e",
    "postinstall": "cd ./node_modules/protractor && npm i webdriver-manager@latest"
  },
  "private": true,
  "dependencies": {
    "@auth0/angular-jwt": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "morgan": "^1.10.0",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.4",
    "rxjs-compat": "~6.5.4",
    "tslib": "^1.10.0",
    "tether": "^1.4.7",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.32",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "htmlhint": "^0.11.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "protractor": "~5.4.3",
    "sass-lint": "^1.13.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
